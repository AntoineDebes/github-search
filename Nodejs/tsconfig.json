{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "outDir": "dist",
    "rootDirs": ["src","./"],
    "esModuleInterop": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "preserveConstEnums": true,
    "allowUnreachableCode": true,

    "moduleResolution": "node",
    "sourceMap": true,
  }
}
